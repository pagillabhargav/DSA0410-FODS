        "import numpy as np\n",
        "import pandas as pd"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "with open(\"sample_text.txt\", \"r\") as file:\n",
        "    text = file.read().lower()\n",
        "for ch in \",.!?;:\":\n",
        "    text = text.replace(ch, \"\")\n",
        "    words = np.array(text.split())\n",
        "freq = pd.Series(words).value_counts()\n",
        "print(freq)"
